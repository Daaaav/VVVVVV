<?xml version="1.0" encoding="UTF-8"?>
<!--
    You can define the plural forms in numbers.xml. More info about this in README.txt

    Then, simply add translations for each form you set up in numbers.xml. For example:

        <translation form="0" translation="Shows up for all numbers with form=0"/>
        <translation form="1" translation="Shows up for all numbers with form=1"/>
        <translation form="2" translation="Shows up for all numbers with form=2"/>
-->
<strings_plural>
    <string english_plural="You rescued %s crewmates" english_singular="You rescued %s crewmate" explanation="These two strings are displayed underneath each other (1/2)" max="40" comment="">
        <translation form="0" translation="Je hebt %s bemanningsleden gered"/>
        <translation form="1" translation="Je hebt %s bemanningslid gered"/>
    </string>
    <string english_plural="and found %s trinkets." english_singular="and found %s trinket." explanation="These two strings are displayed underneath each other (2/2)" max="40" comment="">
        <translation form="0" translation="en %s artefacten gevonden."/>
        <translation form="1" translation="en %s artefact gevonden."/>
    </string>
    <string english_plural="And you found %s trinkets." english_singular="And you found %s trinket." explanation="You rescued all the crewmates! And you found XX trinket(s)." max="38*3" comment="">
        <translation form="0" translation="En je hebt %s artefacten gevonden."/>
        <translation form="1" translation="En je hebt %s artefact gevonden."/>
    </string>
    <string english_plural="%s crewmates remain" english_singular="%s crewmate remains" explanation="" max="40" comment="">
        <translation form="0" translation=""/>
    </string>
</strings_plural>
